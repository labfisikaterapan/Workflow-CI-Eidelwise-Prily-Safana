name: DiabetesClassifier_Eidelwise

python_env: python_env.yaml

entry_points:
  main:
    parameters:
      n_estimators: {type: int, default: 100}
      max_depth: {type: int, default: 10}
      min_samples_split: {type: int, default: 2}
      min_samples_leaf: {type: int, default: 1}
      random_state: {type: int, default: 42}
    command: "python modelling.py --n_estimators {n_estimators} --max_depth {max_depth} --min_samples_split {min_samples_split} --min_samples_leaf {min_samples_leaf} --random_state {random_state}"
